{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["> 1%", "Last 2 versions", "IE >= 10"],
          "node": "current"
        },
        "modules": false,
        "useBuiltIns": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    [
      "import", {
        "libraryName": "antd",
        "libraryDirectory": "lib", 
        "style": false
      }
    ]
  ]
}
